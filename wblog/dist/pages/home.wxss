
.view {
  background-color: #CCEEFF;
}
.title {
  display: flex;
  flex-direction: row;
  font-size: 10px;
  color: #000000;
  margin-bottom: 20rpx;
}
.type {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  background-color: #CCEEFF;
}
.image {
  border: 1px solid #BBFFEE;
  border-radius: 0.8rem;
  margin-top: 30rpx;
  box-shadow: 2px 2px 3px #888888;
}
